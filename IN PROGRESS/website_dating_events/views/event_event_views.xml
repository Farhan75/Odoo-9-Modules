<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_event_form_inherit_website_dating_events" model="ir.ui.view">
            <field name="name">event.dating</field>
            <field name="model">event.event</field>
            <field name="inherit_id" ref="event.view_event_form"/>
            <field name="arch" type="xml">
      	        <notebook position="inside">
      	            <page string="Dating">
      	                <group>
      	                    <field name="dating_event"/>
      	                    <field name="min_age"/>
      	                    <field name="max_age"/>
      	                    <field name="max_male"/>
      	                    <field name="max_female"/>
      	                    <field name="dating_description"/>
      	                </group>
      	            </page>
      	        </notebook>
      	    </field>
        </record>
        
    </data>
</openerp>